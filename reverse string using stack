#include <stack>
#include<string>
#include<stdio.h>
#include<iostream>
using namespace std;

int main(){
    string str="riya";
    stack <char> s;
    for(int i=0;i<str.length();i++){
        s.push(str[i]);
    }
    string ans="";
    while(!s.empty()){
        ans.push_back(s.top());
        s.pop();
    }
    cout<<str<<" after being reversed is :  "<<ans<<endl;
    return 0;

}
